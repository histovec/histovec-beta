{
  "name": "histovec-frontend",
  "version": "1.0.0",
  "private": false,
  "description": "Jouez la transparence en partageant l'historique du véhicule",
  "author": "Ministère de l'Intérieur",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider vite build",
    "dev": "vite",
    "lint": "run-p lint:*",
    "format": "eslint src --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:js": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "start": "NODE_OPTIONS=--openssl-legacy-provider vite",
    "test": "echo 'Tests à écrire'"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.4.1",
    "@gouvminint/vue-dsfr": "1.4.0",
    "base64-arraybuffer": "^1.0.2",
    "chart.js": "^3.8.2",
    "chartjs-adapter-date-fns": "^2.0.0",
    "date-fns": "^2.29.1",
    "dayjs": "^1.11.4",
    "detect-browser": "^5.3.0",
    "lodash.orderby": "^4.6.0",
    "pdf-lib": "^1.17.1",
    "qr.js": "^0.0.0",
    "qrcode.vue": "^3.3.3",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-chartjs": "^4.1.1",
    "vue-router": "^4.1.3",
    "vue3-clipboard": "^1.0.0",
    "vue3-shortkey": "^4.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/test-utils": "2.0.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-csso": "^6.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.7.2",
    "stylelint-config-standard": "^26.0.0",
    "vite": "^3.0.4",
    "vite-svg-loader": "^3.4.0"
  },
  "browserslist": [
    "Chrome >= 49",
    "Firefox >= 45",
    "Safari >= 10",
    "Edge >= 13",
    "ie >= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "engines": {
    "node": "^18.0.0",
    "npm": ">= 8"
  },
  "licence": "GPL"
}

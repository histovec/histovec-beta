{
  "name": "histovec-frontend",
  "version": "1.0.0",
  "private": false,
  "description": "Jouez la transparence en partagean l'historique du véhicule",
  "author": "Ministère de l'Intérieur",
  "scripts": {
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "run-p lint:*",
    "dev": "vue-cli-service serve",
    "dev-ui": "npm-run-all --parallel dev ui",
    "format": "run-s lint:js",
    "inspect": "vue-cli-service inspect",
    "lint:js": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "sb": "npm run storybook:serve",
    "start": "npm run serve",
    "storybook:build": "vue-cli-service storybook:build -c .storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6007 -c .storybook",
    "test:watch": "npm run -s test:unit -- --watch",
    "ui": "vue ui --host 0.0.0.0"
  },
  "dependencies": {
    "@sebastien.nicolas/csa-as-pdf": "^1.0.0",
    "bootstrap": "3.4.1",
    "chart.js": "^2.8.0",
    "core-js": "^3.5.0",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.14",
    "detect-browser": "^4.5.1",
    "es6-promise": "^4.2.8",
    "font-awesome": "^4.7.0",
    "jspdf": "1.4.1",
    "lodash.orderby": "^4.6.0",
    "object-path": "^0.11.4",
    "qrcode.vue": "^1.6.2",
    "regenerator-runtime": "^0.13.2",
    "shake.js": "^1.2.2",
    "vue": "^2.6.10",
    "vue-clipboard2": "^0.3.0",
    "vue-router": "^3.0.6",
    "vue-scrollto": "^2.15.0",
    "vue-shortkey": "^3.1.7",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.1.1",
    "vuex-persist": "2.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-pwa": "^4.1.2",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^25.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "function-bind": "^1.1.1",
    "glob-all": "^3.2.1",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "js-beautify": "1.10.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "purgecss-webpack-plugin": "^1.5.0",
    "sass-loader": "^8.0.0",
    "semver": "^6.1.2",
    "stylelint-config-standard": "^18.3.0",
    "vue-chartjs": "^3.4.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Chrome >= 49",
    "Firefox >= 45",
    "Safari >= 10",
    "Edge >= 13",
    "ie >= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "engines": {
    "node": ">= 11.14.0",
    "npm": ">= 6.9.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "licence": "GPL",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}

{
  "name": "histovec-frontend",
  "version": "1.0.0",
  "private": false,
  "description": "Jouez la transparence en partagean l'historique du véhicule",
  "author": "Ministère de l'Intérieur",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "run-p lint:*",
    "dev": "vue-cli-service serve",
    "dev-ui": "npm-run-all --parallel dev ui",
    "format": "run-s lint:js",
    "inspect": "vue-cli-service inspect",
    "lint:js": "vue-cli-service lint",
    "sb": "npm run storybook:serve",
    "start": "npm run serve",
    "storybook:build": "vue-cli-service storybook:build -c .storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6007 -c .storybook",
    "test:watch": "npm run -s test:unit -- --watch",
    "ui": "vue ui --host 0.0.0.0"
  },
  "dependencies": {
    "base64-arraybuffer": "^1.0.2",
    "bootstrap": "^3.4.1",
    "chart.js": "^2.9.4",
    "core-js": "^3.21.0",
    "dayjs": "^1.10.7",
    "detect-browser": "^5.3.0",
    "es6-promise": "^4.2.8",
    "font-awesome": "^4.7.0",
    "lodash.orderby": "^4.6.0",
    "object-path": "^0.11.8",
    "pdf-lib": "^1.17.1",
    "qr.js": "^0.0.0",
    "qrcode.vue": "^1.7.0",
    "regenerator-runtime": "^0.13.9",
    "shake.js": "^1.2.2",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-clipboard2": "^0.3.3",
    "vue-router": "^3.5.3",
    "vue-scrollto": "^2.20.0",
    "vue-shortkey": "^3.1.7",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-pwa": "4.4.6",
    "@vue/cli-plugin-router": "4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "4.4.6",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob-all": "^3.2.1",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "purgecss-webpack-plugin": "^4.1.3",
    "sass-loader": "^10.1.1",
    "stylelint-config-standard": "^22.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Chrome >= 49",
    "Firefox >= 45",
    "Safari >= 10",
    "Edge >= 13",
    "ie >= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "engines": {
    "node": ">= 11.14.0",
    "npm": ">= 6.9.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "licence": "GPL",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
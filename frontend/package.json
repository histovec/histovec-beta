{
  "name": "histovec-frontend",
  "version": "1.0.0",
  "private": false,
  "description": "Jouez la transparence en partageant l'historique du véhicule",
  "author": "Ministère de l'Intérieur",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider vite build",
    "dev": "vite",
    "stub": "node src/stub/server.js",
    "lint": "run-p lint:*",
    "format": "eslint src --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:js": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "start": "NODE_OPTIONS=--openssl-legacy-provider vite",
    "pretest": "npm run lint",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:stub": "VITE_TITLE=histovec VITE_PORT=8080 VITE_AB_TESTING_PERCENTAGE=0 VITE_DISABLE_API_LOG=false VITE_STUB_ACTIVATED=true BACKEND_PORT=8010 vite",
    "test:cy": "cypress run --config video=false",
    "video:cy": "cypress run"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.9.3",
    "@gouvminint/vue-dsfr": "^4.3.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "axios": "^1.4.0",
    "base64-arraybuffer": "^1.0.2",
    "body-parser": "^1.20.2",
    "chart.js": "^3.8.2",
    "chartjs-adapter-date-fns": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.1",
    "detect-browser": "^5.3.0",
    "express": "^4.18.2",
    "lodash.orderby": "^4.6.0",
    "pdf-lib": "^1.17.1",
    "pinia": "^2.1.3",
    "qr.js": "^0.0.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-chartjs": "^4.1.1",
    "vue-router": "^4.1.3",
    "vue3-clipboard": "^1.0.0",
    "vue3-shortkey": "^4.0.0",
    "whatwg-fetch": "^3.6.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/test-utils": "^2.0.0-alpha.6",
    "aria-mocha": "^0.7.3",
    "aria-vue": "^0.4.1",
    "cypress": "^10.11.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-csso": "^6.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.7.2",
    "stylelint-config-standard": "^26.0.0",
    "vite": "^3.0.4",
    "vite-svg-loader": "^3.4.0",
    "vitest": "^0.24.3"
  },
  "browserslist": [
    "Chrome >= 49",
    "Firefox >= 45",
    "Safari >= 10",
    "Edge >= 13",
    "ie >= 10"
  ],
  "engines": {
    "node": "^18.0.0",
    "npm": ">= 8"
  },
  "licence": "GPL"
}

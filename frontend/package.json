{
  "name": "histovec-frontend",
  "version": "1.0.0",
  "private": false,
  "description": "Jouez la transparence en partageant l'historique du véhicule",
  "author": "Ministère de l'Intérieur",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "run-p lint:*",
    "format": "eslint . --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path '..'",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path '..'",
    "start": "vite",
    "test": "echo 'Tests à écrire'",
    "preview": "run-s build serve",
    "serve": "vite preview"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.4.1",
    "@gouvminint/vue-dsfr": "^1.3.0",
    "base64-arraybuffer": "^1.0.2",
    "chart.js": "^2.9.4",
    "dayjs": "^1.11.2",
    "detect-browser": "^5.3.0",
    "font-awesome": "^4.7.0",
    "lodash.orderby": "^4.6.0",
    "object-path": "^0.11.8",
    "pdf-lib": "^1.17.1",
    "qr.js": "^0.0.0",
    "qrcode.vue": "^3.3.3",
    "shake.js": "^1.2.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.36",
    "vue-chartjs": "^3.5.1",
    "vue-clipboard2": "^0.3.3",
    "vue-router": "^4.0.15",
    "vue-scrollto": "^2.20.0",
    "vue-the-mask": "^0.11.1",
    "vue3-shortkey": "^4.0.0",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "2.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-csso": "^6.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.6.0",
    "stylelint-config-standard": "^25.0.0",
    "vite": "^2.9.9",
    "vite-svg-loader": "^3.3.0"
  },
  "browserslist": [
    "Chrome >= 49",
    "Firefox >= 45",
    "Safari >= 10",
    "Edge >= 13",
    "ie >= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "engines": {
    "node": "^18.0.0",
    "npm": ">= 8"
  },
  "licence": "GPL"
}

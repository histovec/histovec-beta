{
  "name": "histovec-frontend",
  "version": "1.0.0",
  "private": false,
  "description": "Jouez la transparence en partagean l'historique du véhicule",
  "author": "Ministère de l'Intérieur",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider vite build",
    "dev": "vite",
    "lint": "run-p lint:*",
    "format": "eslint . --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "start": "NODE_OPTIONS=--openssl-legacy-provider vite"
  },
  "dependencies": {
    "base64-arraybuffer": "^1.0.2",
    "bootstrap": "^3.4.1",
    "chart.js": "^2.9.4",
    "dayjs": "^1.11.1",
    "detect-browser": "^5.3.0",
    "font-awesome": "^4.7.0",
    "lodash.orderby": "^4.6.0",
    "object-path": "^0.11.8",
    "pdf-lib": "^1.17.1",
    "qr.js": "^0.0.0",
    "qrcode.vue": "^1.7.0",
    "shake.js": "^1.2.2",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-clipboard2": "^0.3.3",
    "vue-router": "^3.5.3",
    "vue-scrollto": "^2.20.0",
    "vue-shortkey": "^3.1.7",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.3.0",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss-csso": "^6.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.4",
    "stylelint-config-standard": "^25.0.0",
    "vite": "^2.9.6",
    "vite-plugin-vue2": "^1.9.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Chrome >= 49",
    "Firefox >= 45",
    "Safari >= 10",
    "Edge >= 13",
    "ie >= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "engines": {
    "node": "^18.0.0",
    "npm": ">= 8"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "licence": "GPL",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
{
  "name": "histovec-backend",
  "version": "1.0.0",
  "description": "Jouez la transparence en partageant l'historique du véhicule",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev-setup": "node dev-setup/index.js",
    "dev": "nodemon --inspect --watch src boot-dev.js",
    "format": "eslint src --fix",
    "prelint": "npm run format",
    "lint": "eslint src",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:only": "cross-env NODE_ENV=test jest",
    "pretest": "npm run lint",
    "test:all": "run-s test",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --detectOpenHandles --forceExit"
  },
  "keywords": [
    "vehicule",
    "occasion",
    "certificat",
    "immatriculation"
  ],
  "author": "Ministère de l'intérieur",
  "license": "GPLv3",
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.1",
    "luxon": "^1.26.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.0",
    "redis": "^3.1.2",
    "util": "^0.12.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/register": "^7.13.16",
    "babel-jest": "^26.6.3",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "html-loader": "^2.1.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.7",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:all"
    }
  },
  "lint-staged": {
    "linters": {
      "../**/*.{js,vue,htm,html,css,scss}": [
        "npm run lint",
        "git add"
      ]
    }
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "plugin:import/errors",
      "plugin:import/warnings",
      "standard"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "import/no-unresolved": [
        2,
        {
          "commonjs": true,
          "amd": true
        }
      ],
      "import/named": 2,
      "import/namespace": 2,
      "import/default": 2,
      "import/export": 2,
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  }
}
